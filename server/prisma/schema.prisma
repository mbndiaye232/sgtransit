  IDDEVISEFOB                            BigInt?                @default(0)
  IDDEVISEFRET                           BigInt?                @default(0)
  IDDEVISEASS                            BigInt?                @default(0)
  CAF                                    Decimal?               @default(0.000000) @db.Decimal(24, 6)
  CodeRegimeDeclaration                  String?                @default("0,00") @db.VarChar(10)
  MontantTaxes                           Decimal?               @default(0.000000) @db.Decimal(24, 6)
  NTS                                    String?                @db.VarChar(15)
  FOBCFA                                 Decimal?               @default(0.000000) @db.Decimal(24, 6)
  FRETCFA                                Decimal?               @default(0.000000) @db.Decimal(24, 6)
  ASSURANCESCFA                          Decimal?               @default(0.000000) @db.Decimal(24, 6)
  IdAgent                                BigInt?                @default(0)
  DPI                                    String?                @db.VarChar(50)
  NBCOLIS                                Int?                   @default(0)
  TitreExo                               String?                @db.VarChar(150)
