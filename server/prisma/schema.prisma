generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model agents {
  IDAgents                                    BigInt                      @id @default(autoincrement())
  NomAgent                                    String?                     @db.VarChar(100)
  Sexe                                        String?                     @db.VarChar(50)
  Datenaiss                                   DateTime?                   @db.Date
  Lieunaiss                                   String?                     @db.VarChar(75)
  adresse                                     String?                     @db.VarChar(75)
  Tel                                         String?                     @db.VarChar(25)
  Cel                                         String?                     @db.VarChar(25)
  Email                                       String?                     @db.VarChar(100)
  FonctionAgent                               String?                     @db.VarChar(75)
  Observations                                String?                     @db.VarChar(250)
  photo                                       Bytes?
  cheminphoto                                 String?                     @db.VarChar(255)
  Login                                       String?                     @unique(map: "Login") @db.VarChar(150)
  ValidationDossiers                          Int?                        @default(0) @db.TinyInt
  Niveauhabilitation                          Decimal?                    @default(0.000000) @db.Decimal(24, 6)
  PayeParCaisse                               Int?                        @default(0) @db.TinyInt
  ValidationPieceCaisse                       Int?                        @default(0) @db.TinyInt
  ValiderNoteDetail                           Int?                        @default(0) @db.TinyInt
  MajTEC                                      Int?                        @default(0) @db.TinyInt
  clients                                     clients[]
  documents                                   documents[]
  dossiers_dossiers_IdAgentSaisiToagents      dossiers[]                  @relation("dossiers_IdAgentSaisiToagents")
  dossiers_dossiers_IdAgModiffToagents        dossiers[]                  @relation("dossiers_IdAgModiffToagents")
  dossiers_dossiers_IdAgentValidationToagents dossiers[]                  @relation("dossiers_IdAgentValidationToagents")
  factures                                    factures[]
  liaisonagentsgroupes                        liaisonagentsgroupes[]
  liaisondossiersdeclarants                   liaisondossiersdeclarants[]
  ordrestransit                               ordrestransit[]
  reglements                                  reglements[]
  transports                                  transports[]

  @@index([NomAgent], map: "WDIDX_Agents_NomAgent")
}

model articles {
  IDArticles                             BigInt                 @id @default(autoincrement())
  IDNotesDeDetails                       BigInt?                @default(0)
