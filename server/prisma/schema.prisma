  @@index([CodeRegimeDeclaration], map: "WDIDX_Articles_CodeRegimeDeclaration")
  @@index([DPI], map: "WDIDX_Articles_DPI")
  @@index([IDDEVISEASS], map: "WDIDX_Articles_IDDEVISEASS")
  @@index([IDDEVISEFOB], map: "WDIDX_Articles_IDDEVISEFOB")
  @@index([IDDEVISEFRET], map: "WDIDX_Articles_IDDEVISEFRET")
  @@index([IDNotesDeDetails], map: "WDIDX_Articles_IDNotesDeDetails")
  @@index([IDRegimeDeclaration], map: "WDIDX_Articles_IDRegimeDeclaration")
  @@index([IdAgent], map: "WDIDX_Articles_IdAgent")
  @@index([NTS], map: "WDIDX_Articles_NTS")
  @@index([NumeroArticle], map: "WDIDX_Articles_NumeroArticle")
  @@index([Origine], map: "WDIDX_Articles_Origine")
  @@index([TitreExo], map: "WDIDX_Articles_TitreExo")
}

