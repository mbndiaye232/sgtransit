  devises_articles_IDDEVISEFRETTodevises devises?               @relation("articles_IDDEVISEFRETTodevises", fields: [IDDEVISEFRET], references: [IDDevises], onDelete: NoAction, onUpdate: NoAction, map: "articles_ibfk_2")
  devises_articles_IDDEVISEASSTodevises  devises?               @relation("articles_IDDEVISEASSTodevises", fields: [IDDEVISEASS], references: [IDDevises], onDelete: NoAction, onUpdate: NoAction, map: "articles_ibfk_3")
